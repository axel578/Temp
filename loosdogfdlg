  - modifytargetscore{objective=fire;action=set;value=1} @self
  - sound{s=block.fire.ambient;v=2;repeat=60;repeatInterval=5} @self #NONEED
  - sound{s=entity.zombie_horse.ambient;v=1;repeat=60;repeatInterval=5} @self #NONEED
  - setblock{m=FIRE;md=0;repeat=60;repeatInterval=5} @selflocation #NONEED
  - effect:particles{p=lava;amount=5;speed=0.1;hS=0.3;vS=0.3;repeat=60;repeatInterval=5} @self
  - effect:particles{p=flame;amount=10;speed=0.1;hS=0.3;vS=0.3;repeat=60;repeatInterval=5} @self
  - ignite{ticks=30;repeat=30;repeatInterval=10} @self #NONEED
  - potion{type=SPEED;duration=20;level=2;force=true;particles=false;repeat=30;repeatInterval=10} @self  # !! YES !! repeat=30 ==> Doit être égal au (Delay / 10) de la ligne 264 (Exemple ici: 300/10 = 30) => Augmente la durée de l'armure de feu
  - delay 300 # !! YES !! delay 300 ==> Cf ligne au dessus
  - modifytargetscore{objective=fire;action=set;value=0} @self
  
ArmorFire1:
  Conditions:
  - score{objective=fire;value=1} true
  Skills:
  - ignite{ticks=80} @trigger
  - damage{amount=1;element=FIRE;pkb=true;ia=true} @trigger # YES
