CerberusCombo1:
  LegacyConditions:
  - stance Cerberus
  - mobscore CerbCombo1;=1
  Skills:
  - modifymobscore{o=UnsetRedOrb;a=SET;v=1}
  - potion{type=SLOW;duration=500;level=3} @self
  - potion{type=DAMAGE_RESISTANCE;duration=30;level=10} @self
  - effect:particlering{p=sweepAttack;amount=1;radius=1;points=8;hS=0.1;vS=0.1;yOffset=0.7} @self
  - effect:particlering{p=sweepAttack;amount=1;radius=1;points=8;hS=0.1;vS=0.1;yOffset=0.5} @self
  - effect:particlering{p=sweepAttack;amount=1;radius=1;points=8;hS=0.1;vS=0.1;yOffset=0.3} @self
