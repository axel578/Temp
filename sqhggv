  - cure{t=INCREASE_DAMAGE} @self  
  
CerberusCombo1DamageCalculation:
  Skills:
  - skill{s=CerberusCombo1DamageLevel1}
  - skill{s=CerberusCombo1DamageLevel2}
  - skill{s=CerberusCombo1DamageLevel3}
CerberusCombo1DamageLevel1:
  LegacyConditions:
  - mobscore CerberusLevel;=1
  Skills:
  - damage{a=3}
CerberusCombo1DamageLevel2:
  LegacyConditions:
  - mobscore CerberusLevel;=2
  Skills:
  - damage{a=5}
CerberusCombo1DamageLevel3:
  LegacyConditions:
  - mobscore CerberusLevel;=3
  Skills:
  - damage{a=7}

Sweep-CerberusCombo1DamageCalculation:
  Skills:
  - skill{s=Sweep-CerberusCombo1DamageLevel1}
  - skill{s=Sweep-CerberusCombo1DamageLevel2}
  - skill{s=Sweep-CerberusCombo1DamageLevel3}
