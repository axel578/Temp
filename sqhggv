BuyCerberusLevel2:
  LegacyConditions:
  - mobscore CerberusLevel;=1
  - mobscore RedO;>1001
  Skills:
  - sendtitle{title="&cAlert!";subtitle="<mob.name> &eupgraded &cCerberus &eto &cLevel II";d=60} @PlayersInRadius{r=40}
  - message{m="&7[&l&cAlert!&r&7] &4<mob.name> &eupgraded &cCerberus &eto &cLevel II"} @PlayersInRadius{r=40}
  - modifymobscore{o=CerberusLevel;a=SET;v=2}
