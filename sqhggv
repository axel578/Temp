levitateplant_grow:
  Skills:
  - partvisibility{m=levitateplant;p=bone;v=false} @self
  - partvisibility{m=levitateplant;p=bone2;v=false} @self
  - delay 200
  - partvisibility{m=levitateplant;p=bone;v=true} @self
  - delay 200
  - partvisibility{m=levitateplant;p=bone2;v=true} @self
  - setstance{stance=grown} @self
 
levitateplant_dying:
  Cooldown: 5
  Skills:
  - skill{s=levitateplant_die} @self 0.08 
 
levitateplant_die:
  Skills:
  - message{m="&cSadly the plant died from using it too much!"} @EntitiesInRadius{r=10}
  - sound{s=block.crop.break} @self 
  - model{mid=levitateplant;r=true} @self   
  - remove @self  
 

fireplant_effect:
  Conditions:
  - stance grown
  Skills:
  - effect:particles{p=flame;a=1;hs=0;vs=0;s=0.01;forwardOffset=0;sideOffset=-0.1;yOffset=0.5} @self
  - delay 1
  - effect:particles{p=flame;a=1;hs=0;vs=0;s=0.01;forwardOffset=0;sideOffset=-0.2;yOffset=0.6} @self
  - delay 1
  - effect:particles{p=flame;a=2;hs=0;vs=0;s=0.01;forwardOffset=0;sideOffset=-0.3;yOffset=0.6} @self
  - effect:particles{p=lava;a=2;hs=0;vs=0;s=0.1;forwardOffset=0;sideOffset=-0.3;yOffset=0.6} @self
  - delay 1
  - effect:particles{p=flame;a=2;hs=0;vs=0;s=0.01;forwardOffset=0;sideOffset=-0.4;yOffset=0.7} @self
  - delay 1
  - effect:particles{p=flame;a=3;hs=0;vs=0;s=0.01;forwardOffset=0;sideOffset=-0.5;yOffset=0.7} @self
  - effect:particles{p=lava;a=2;hs=0;vs=0;s=0.1;forwardOffset=0;sideOffset=-0.5;yOffset=0.7} @self
  - delay 1
  - effect:particles{p=flame;a=6;hs=0;vs=0;s=0.01;forwardOffset=0;sideOffset=-0.6;yOffset=0.8} @self
  - ignite{ticks=100} @EntitiesInRadius{r=2}
  - skill{s=fireplant_explode} @self

fireplant_explode:
  Cooldown: 5
  Conditions:
  - playerwithin{d=2} true
  Skills:
  - skill{s=fireplant_exploding} @self 0.08 


fireplant_exploding:
  Skills:
  - ignite{ticks=150} @EntitiesInRadius{r=4}  
  - effect:particles{p=reddust;c=#0ac6cc;a=20;hs=1.5;vs=1;s=0.5;forwardOffset=0;sideOffset=0;yOffset=0.8}
  - throw{velocity=8;velocityY=5} @EntitiesInRadius{r=3}  
  - sound{s=block.fungus.break} @self
  - sound{s=block.fungus.place} @self
  - message{m="&cSadly the plant died from using it too much!"} @EntitiesInRadius{r=10} 
  - model{mid=fireplant;r=true} @self ~onDeath   
  - remove @self  


fireplant_grow:
  Skills:
  - partvisibility{m=fireplant;p=bone;v=false} @self
  - delay 200
  - partvisibility{m=fireplant;p=bone;v=true} @self
  - setstance{stance=grown} @self



iceplant_effect:
  Skills:
  - potion{type=SLOW_DIGGING;duration=300;level=1;p=false} @EntitiesInRadius{r=1}
  - effect:particles{p=reddust;c=#EEFFFE;a=2;hs=0.1;vs=0;s=0;forwardOffset=0;sideOffset=0;yOffset=0.4}

iceplant_nointeract:
  Skills:
  - sudoskill{s=iceplant_nointeractmessage} @trigger

iceplant_nointeractmessage:
  Conditions:
  - holding{m=water_bucket;s=MainHand} false
  Skills:
  - message{m="&eUse a Water Bucket on this plant!"} @self  


iceplant_interact:
  Skills:
  - sudoskill{s=get_iceblock} @trigger

get_iceblock:
  Conditions:
  - holding{m=water_bucket;s=MainHand} true
  Skills:
  - consumeHeldItem{amount=1}
  - sound{s=item.bucket.empty} @self
  - sound{s=block.glass.hit} @self    
  - randomskill{
    skills=
    iceblock1,
    iceblock2,
    }

iceplant_death:
 Skills:
  - sound{s=block.glass.break} @self
  - message{m="&cSadly the plant died from using it too much!"} @EntitiesInRadius{r=5}  
  - model{mid=iceplant;r=true} @self ~onDeath     
  - remove @self

iceblock1:
  Skills:
  - effect:itemspray{item=ice;amount=4;velocity=0.2;vv=2;d=60;yo=1.5;ap=true} @target
  - effect:itemspray{item=bucket;amount=1;velocity=0.2;vv=2;d=60;yo=1.5;ap=true} @target  
  - sudoskill{s=iceplant_death} @target 0.02  

iceblock2:
  Skills:
  - effect:itemspray{item=packed_ice;amount=4;velocity=0.2;vv=2;d=60;yo=1.5;ap=true} @target
  - effect:itemspray{item=bucket;amount=1;velocity=0.2;vv=2;d=60;yo=1.5;ap=true} @target   
  - sudoskill{s=iceplant_death} @target 0.04  
