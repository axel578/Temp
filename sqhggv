  - sudoskill{s=get_potion} @trigger

get_potion:
  Conditions:
  - holding{m=glass_bottle;s=MainHand} true
  Skills:
  - consumeHeldItem{amount=1}
  - sound{s=block.brewing_stand.brew} @self
  - sudoskill{s=get_exhausted} @target   
  - randomskill{
    skills=
    potion1,
    potion2,
    potion3,
    potion4
    }

