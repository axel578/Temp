CerberusPChain1Execute:
  Skills:
  - castif{c="mobscore CerberusPChain;=1 && mobscore CerbCombo1;=0";meet=BuyCerberusCombo1;else=BuyCerberusCombo2}
  - castif{c="mobscore CerberusPChain;=1 && mobscore CerbCombo1;=1 && mobscore CerbCombo2;=0";meet=BuyCerberusCombo2;else=BuyCerberusLevel2}
  - castif{c="mobscore CerberusPChain;=1 && mobscore CerbCombo1;=1 && mobscore CerbCombo2;=1 && mobscore CerberusLevel;=1";meet=BuyCerberusLevel2;else=BuyCerberusSwing}
  - castif{c="mobscore CerberusPChain;=1 && mobscore CerbCombo1;=1 && mobscore CerbCombo2;=1 && mobscore CerberusLevel;=2 && mobscore CSwing;=0";meet=BuyCerberusSwing;else=BuyCerberusRevolver}
  - castif{c="mobscore CerberusPChain;=1 && mobscore CerbCombo1;=1 && mobscore CerbCombo2;=1 && mobscore CerberusLevel;=2 && mobscore CSwing;=1 && mobscore CRevolver;=0";meet=BuyCerberusRevolver;else=BuyCerberusLevel3}
  - castif{c="mobscore CerberusPChain;=1 && mobscore CerbCombo1;=1 && mobscore CerbCombo2;=1 && mobscore CerberusLevel;=2 && mobscore CSwing;=1 && mobscore CRevolver;=1";meet=BuyCerberusLevel3;else=BuyCerberusWindmill}
  - castif{c="mobscore CerberusPChain;=1 && mobscore CerbCombo1;=1 && mobscore CerbCombo2;=1 && mobscore CerberusLevel;=3 && mobscore CSwing;=1 && mobscore CRevolver;=1 && mobscore CWindmill;=0";meet=BuyCerberusWindmill;else=BuyCerberusSatellite}
  - castif{c="mobscore CerberusPChain;=1 && mobscore CerbCombo1;=1 && mobscore CerbCombo2;=1 && mobscore CerberusLevel;=3 && mobscore CSwing;=1 && mobscore CRevolver;=1 && mobscore CWindmill;=1 && mobscore CSatellite;=0";meet=BuyCerberusSatellite;else=BuyCerberusCombo1}

