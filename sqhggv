  - effect:particlering{p=instantSpell;amount=5;radius=1;points=8;hS=0.1;vS=0.1;yOffset=0.7} @self
  - effect:particles{p=instantSpell;amount=20;hS=1.1;vS=0.5;speed=0;y=0.5} @self
  - effect:particlering{p=instantSpell;amount=5;radius=1;points=8;hS=0.1;vS=0.1;yOffset=0.5} @self
  - effect:particlering{p=instantSpell;amount=5;radius=1;points=8;hS=0.1;vS=0.1;yOffset=0.3} @self
  - skill{s=Sweep-CerberusCombo1DamageCalculation} @EIR{r=3}
  - delay 15
  - potion{type=DAMAGE_RESISTANCE;duration=30;level=10} @self
