  - effect:particlering{p=instantSpell;amount=5;radius=1;points=8;hS=0.1;vS=0.1;yOffset=0.5} @self
  - skill{s=LuciferComboEDamageCalculation} @LivingInCone{a=90.0;r=3.0;rot=0.0} 
  - summon{t=LuciferSwordA;a=2;r=1} @self
  - delay 12
  - leap{velocity=100} @target
  - message{m="&4&l<mob.name> &7> &6And in the end...we are all satisfied..."} @PIR{r=20}
  - sound{s=entity.player.attack.sweep} @self
  - effect:particlering{p=instantSpell;amount=5;radius=1;points=8;hS=0.1;vS=0.1;yOffset=0.5} @self
  - skill{s=LuciferComboEDamageCalculation} @LivingInCone{a=90.0;r=3.0;rot=0.0} 
  - summon{t=LuciferSwordA;a=2;r=1} @self
  - delay 30
  - leap{velocity=300} @target
  - sound{s=entity.player.attack.sweep} @self
  - effect:particlering{p=instantSpell;amount=5;radius=1;points=8;hS=0.1;vS=0.1;yOffset=0.5} @self
  - skill{s=LuciferComboCDamageCalculation} @LivingInCone{a=90.0;r=3.0;rot=0.0} 
  - summon{t=LuciferSwordC;a=3;r=2} @self
  - delay 80
  - message{m="&4&l<mob.name> &7> &6And you are set free."} @PIR{r=20}
  - signal{s=LDetonate} @MIR{r=50;t=LuciferSwordA,LuciferSwordC}
  - modifymobscore{o=UnsetRedOrb;a=SET;v=0}
  - cure{t=SLOW} @self
  - cure{t=INCREASE_DAMAGE} @self    
  
