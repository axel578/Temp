  Skills:
  - Aura{auraName=mexbot_walk_animation-Aura;onTick=mexbot_walk_animation-Tick;interval=2;d=916000000} @self
  
mexbot_walk_animation-Tick:
  Conditions:
  - altitude{h=>1} castinstead mexbot_walk_animation_fly-Tick
  - hastag{t=ground} false
  - hastag{t=fly} true
  Skills:
  - addtag{t=ground} @self
  - removetag{t=fly} @self
  - defaultstate{t=idle;s=idle} @self
  - defaultstate{t=walk;s=walk} @self

