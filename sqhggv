  - sound{s=entity.player.attack.sweep} @self
  - effect:particlering{p=reddust;color=#00FFFF;amount=40;radius=2;points=20;hS=0.5;vS=0.2;yOffset=0.7} @self
  - effect:particles{p=instantSpell;amount=20;hS=1.1;vS=0.5;speed=0;y=0.5} @self
  - effect:particlering{p=reddust;color=#00FFFF;amount=40;radius=2;points=20;hS=0.5;vS=0.2;yOffset=0.5} @self
  - effect:particlering{p=reddust;color=#00FFFF;amount=40;radius=2;points=20;hS=0.5;vS=0.2;yOffset=0.3} @self
  - skill{s=Sweep-CerberusSatelliteDamageCalculation} @EIR{r=3}
  
  - modifymobscore{o=UnsetRedOrb;a=SET;v=0}
  
  - cure{t=SLOW} @self
  - cure{t=INCREASE_DAMAGE} @self

  

